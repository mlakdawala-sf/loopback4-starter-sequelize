{
  "name": "loopback4-starter-sequelize",
  "version": "0.0.1",
  "description": "Open source",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "cross-env NODE_ENV=test npm run mocha-test",
    "mocha-test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t loopback4-starter-sequelize .",
    "docker:run": "docker run -p 3000:3000 -d loopback4-starter-sequelize",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "npm run build && node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "dev": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "db:migrate": "./node_modules/.bin/db-migrate up --config './migrations/database.json'",
    "db:migrate:down": "./node_modules/.bin/db-migrate down --config './migrations/database.json'",
    "db:migrate:reset": "./node_modules/.bin/db-migrate reset --config './migrations/database.json'"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "mudassir lakdawala <mudasssir.lakdawala@sourcefuse.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@alt3/sequelize-to-json-schemas": "^0.3.55",
    "@loopback/boot": "^3.3.1",
    "@loopback/core": "^2.15.1",
    "@loopback/repository": "^3.5.1",
    "@loopback/rest": "^9.2.1",
    "@loopback/rest-explorer": "^3.2.1",
    "@loopback/service-proxy": "^3.1.1",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.9.0",
    "jsonwebtoken": "^8.5.1",
    "kv-redis": "^0.1.3",
    "loopback-connector-kv-redis": "^4.0.0",
    "loopback4-authentication": "^4.3.0",
    "loopback4-authorization": "^3.2.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "sqlite3": "^5.0.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.1",
    "@loopback/eslint-config": "^10.1.1",
    "@loopback/testlab": "^3.3.1",
    "@types/bcrypt": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^10.17.58",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "eslint": "^7.23.0",
    "source-map-support": "^0.5.19",
    "tsc-watch": "^4.2.9",
    "typescript": "~4.2.3"
  }
}
